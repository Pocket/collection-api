{
  "name": "collection-api",
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w & nodemon",
    "start": "npm run migrate:deploy && node dist/main.js",
    "start:dev": "npm run migrate:deploy && npm run build && npm run watch",
    "test-ci": "npm test",
    "test": "jest \"\\.spec\\.ts\"",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "lint-check": "eslint --fix-dry-run \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "db:generate-client": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset"
  },
  "prisma": {
    "seed": "ts-node --emit=false prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/federation": "0.36.1",
    "@pocket-tools/apollo-utils": "2.2.0",
    "@prisma/client": "3.10.0",
    "@sentry/node": "7.7.0",
    "@sentry/tracing": "7.7.0",
    "apollo-server-cache-redis": "3.3.1",
    "apollo-server-core": "3.10.0",
    "apollo-server-errors": "3.3.1",
    "apollo-server-express": "3.10.0",
    "apollo-server-plugin-base": "3.6.2",
    "apollo-server-plugin-response-cache": "3.7.0",
    "apollo-server-types": "3.6.2",
    "aws-sdk": "2.1092.0",
    "aws-xray-sdk-core": "3.3.6",
    "aws-xray-sdk-express": "3.3.6",
    "graphql": "15.8.0",
    "graphql-upload": "13.0.0",
    "slugify": "1.6.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "6.3.1",
    "@pocket-tools/eslint-config": "2.0.0",
    "@types/chai": "4.3.1",
    "@types/graphql-upload": "8.0.11",
    "@types/jest": "28.1.6",
    "@types/node": "16.11.42",
    "chai": "4.3.6",
    "jest": "28.1.3",
    "nock": "13.2.9",
    "nodemon": "2.0.19",
    "prisma": "3.10.0",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}

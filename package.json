{
  "name": "collection-api",
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w & nodemon",
    "start": "npm run migrate:deploy && node dist/main.js",
    "start:dev": "npm run migrate:deploy && npm run build && npm run watch",
    "test-ci": "npm test",
    "test": "jest \"\\.spec\\.ts\"",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "lint-check": "eslint --fix-dry-run \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "db:generate-client": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "db:seed": "prisma db seed --preview-feature"
  },
  "dependencies": {
    "@apollo/federation": "^0.33.3",
    "@pocket-tools/apollo-utils": "^2.0.0",
    "@prisma/client": "^2.30.3",
    "@sentry/node": "^6.13.3",
    "@sentry/tracing": "^6.13.0",
    "apollo-server": "^3.4.0",
    "apollo-server-cache-redis": "^3.2.0",
    "apollo-server-express": "^3.3.0",
    "apollo-server-plugin-response-cache": "^3.3.0",
    "aws-sdk": "^2.1017.0",
    "aws-xray-sdk-core": "^3.3.3",
    "aws-xray-sdk-express": "^3.3.3",
    "graphql": "^15.7.2",
    "graphql-upload": "^12.0.0",
    "mime-types": "^2.1.33",
    "slugify": "^1.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/faker": "^5.5.9",
    "@types/graphql-upload": "^8.0.7",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "chai": "^4.3.4",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^27.3.1",
    "nock": "^13.1.4",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "prisma": "^2.30.3",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}

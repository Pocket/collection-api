{
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w & nodemon",
    "start": "npm run migrate:deploy && node dist/main.js",
    "start:dev": "npm run migrate:deploy && npm run build && npm run watch",
    "test-ci": "npm test",
    "test": "jest \"\\.spec\\.ts\"",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "lint-check": "eslint --fix-dry-run \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "db:generate-client": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "db:seed": "prisma db seed --preview-feature"
  },
  "dependencies": {
    "@apollo/federation": "^0.25.1",
    "@pocket-tools/apollo-utils": "^1.0.4",
    "@prisma/client": "^2.23.0",
    "@sentry/node": "^6.4.1",
    "@sentry/tracing": "^6.4.1",
    "apollo-server": "^2.25.0",
    "apollo-server-cache-redis": "^1.5.0",
    "apollo-server-express": "^2.25.0",
    "apollo-server-plugin-response-cache": "^0.9.0",
    "aws-sdk": "^2.917.0",
    "aws-xray-sdk-core": "^3.3.3",
    "aws-xray-sdk-express": "^3.3.3",
    "graphql": "^15.5.0",
    "graphql-upload": "^12.0.0",
    "mime-types": "^2.1.30",
    "slugify": "^1.5.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/faker": "^5.5.5",
    "@types/graphql-upload": "^8.0.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "chai": "^4.3.4",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.5.3",
    "jest": "^27.0.1",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "prisma": "^2.23.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.2"
  }
}

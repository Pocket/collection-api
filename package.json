{
  "name": "collection-api",
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w & nodemon",
    "start": "npm run migrate:deploy && node dist/main.js",
    "start:dev": "npm run migrate:deploy && npm run build && npm run watch",
    "test-ci": "npm test",
    "test": "jest \"\\.spec\\.ts\" --runInBand",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "lint-check": "eslint --fix-dry-run \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "db:generate-client": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset"
  },
  "prisma": {
    "seed": "ts-node --emit=false prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "4.9.0",
    "@apollo/server-plugin-landing-page-graphql-playground": "4.0.1",
    "@apollo/server-plugin-response-cache": "4.1.3",
    "@apollo/subgraph": "2.5.1",
    "@aws-sdk/client-eventbridge": "^3.276.0",
    "@pocket-tools/apollo-utils": "3.0.0",
    "@prisma/client": "^4.16.2",
    "@sentry/node": "7.60.1",
    "@sentry/tracing": "7.60.1",
    "aws-sdk": "2.1423.0",
    "aws-xray-sdk-core": "3.5.1",
    "aws-xray-sdk-express": "3.5.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "graphql-upload": "15.0.2",
    "slugify": "1.6.6",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@pocket-tools/eslint-config": "2.0.0",
    "@types/chai": "4.3.5",
    "@types/jest": "29.5.3",
    "@types/node": "18.17.1",
    "@types/sinon": "^10.0.13",
    "chai": "4.3.7",
    "dotenv": "16.3.1",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "nock": "13.3.2",
    "nodemon": "2.0.22",
    "prisma": "^4.16.2",
    "sinon": "^15.0.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
